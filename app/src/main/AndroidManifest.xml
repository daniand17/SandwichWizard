<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sandwichwizard.sandwichguys.sandwichwizard">

    <application
        android:allowBackup="true"
        android:icon="@drawable/fire_wiz"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="QuizActivity"
            android:label="@string/app_name"></activity>
        <activity
            android:name="ShakerActivity"
            android:label="@string/title_activity_shaker"></activity>
        <activity
            android:name="CustomizationActivity"
            android:label="@string/title_activity_customization"></activity>
    </application>

</manifest>
